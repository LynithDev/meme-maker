{
    "extends": [
        "../../tsconfig.json",
        "./.svelte-kit/tsconfig.json"
    ],
    "compilerOptions": {
        "experimentalDecorators": true,
        "moduleResolution": "bundler",
        "resolveJsonModule": true,
        "allowJs": true,
        "checkJs": true,
        "strict": true,
        "sourceMap": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true
    }
}
